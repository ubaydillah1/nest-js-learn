generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       String @id @default(uuid())
  username String
  email    String @unique
  password String
  role     ROLE   @default(USER)
}

enum ROLE {
  ADMIN
  USER
}
